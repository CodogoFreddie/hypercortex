{
	"title": "hypercortex",
	"version": "0.0.1",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-eslint": "^9.0.0",
		"babel-preset-freddie-ridell": "0.0.8",
		"eslint": "^5.9.0",
		"eslint-config-react-app": "^2.1.0",
		"eslint-plugin-flowtype": "^2.50.3",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^6.1.2",
		"eslint-plugin-react": "^7.11.1",
		"lerna": "^3.0.0-rc.0",
		"prettier": "^1.15.3"
	},
	"babel": {
		"presets": [
			"freddie-ridell"
		]
	},
	"bin": {
		"null": "./main.js"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"files": [
		"/main.js"
	],
	"prettier": {
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"scripts": {
		"build": "lerna run build --parallel --concurrency 4",
		"format": "lerna run format --parallel",
		"test": "lerna run test",
		"clean": "lerna run clean --parallel",
		"watch": "lerna run watch --parallel --stream",
		"pub": "npm run clean && npm run build && npm run format && lerna publish"
	},
	"name": "hypercortex",
	"dependencies": {
		"chrono-node": "^1.3.5",
		"object-hash": "^1.3.1"
	}
}
