{
	"title": "hypercortex",
	"version": "0.0.1",
	"devDependencies": {
		"lerna": "^3.13.1"
	},
	"babel": {
		"presets": [
			"freddie-ridell"
		]
	},
	"bin": {
		"null": "./main.js"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"files": [
		"/main.js"
	],
	"prettier": {
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"scripts": {
		"build": "lerna run build --parallel --concurrency 4",
		"format": "lerna run format --parallel",
		"test": "lerna run test",
		"clean": "lerna run clean --parallel",
		"watch": "lerna run watch --parallel --stream",
		"pub": "npm run clean && npm run build && npm run format && lerna publish"
	},
	"name": "hypercortex",
	"dependencies": {
		"chrono-node": "^1.3.5",
		"object-hash": "^1.3.1"
	}
}
