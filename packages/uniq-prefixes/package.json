{
  "name": "@hypercortex/uniq-prefixes",
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "version": "0.6.13",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@freddieridell/babel-preset": "^0.1.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.3",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest-cli": "^24.5.0",
    "prettier": "^1.16.4"
  },
  "babel": {
    "presets": [
      "@freddieridell/babel-preset"
    ]
  },
  "eslintConfig": {
