{
  "name": "@hypercortex/hyperpin-cli",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.6.16",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-freddie-ridell": "0.0.8",
    "eslint": "^5.5.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^1.14.2"
  },
  "babel": {
    "presets": [
      "freddie-ridell"
    ]
  },
  "bin": {
    "pin": "./main.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "files": [
    "/main.js",
    "/index.js",
    "/lib"
