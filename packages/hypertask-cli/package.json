{
	"name": "@hypercortex/hypertask-cli",
	"publishConfig": {
		"access": "public"
	},
	"version": "0.0.3",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-eslint": "^9.0.0",
		"babel-preset-freddie-ridell": "0.0.8",
		"eslint": "^5.5.0",
		"eslint-config-react-app": "^2.1.0",
		"eslint-plugin-flowtype": "^2.50.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.11.1",
		"prettier": "^1.14.2"
	},
	"babel": {
		"presets": [
			"freddie-ridell"
		]
	},
	"bin": {
		"task": "./main.js"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"files": [
		"/main.js"
	],
	"prettier": {
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"scripts": {
		"build": "babel src --out-dir lib",
		"format": "prettier --write src/**/*",
		"watch": "babel src --out-dir lib --watch"
	},
	"dependencies": {
		"@hypercortex/hypertask": "^0.0.3",
		"@hypercortex/wrapper": "^0.0.3",
		"chrono-node": "^1.3.5",
		"date-fns": "^2.0.0-alpha.16",
		"env-paths": "^1.0.0",
		"hypercore-crypto": "^1.0.0",
		"hyperdb": "^3.5.0",
		"inquirer": "^6.2.0",
		"rimraf": "^2.6.2"
	}
}
