  "bugs": {
    "url": "https://github.com/CodogoFreddie/hypercortex/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-freddie-ridell": "0.0.8",
    "eslint": "^5.9.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^1.15.3"
  },
  "babel": {
    "presets": [
      "freddie-ridell"
    ]
  },
  "eslintConfig": {
